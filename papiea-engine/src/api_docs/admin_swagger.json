            "paths": {
                "/provider/": {
                    "post": {
                        "description": "Creates a new provider",
                        "operationId": "addprovider",
                        "tags": ["/provider"],
                        "requestBody": {
                            "description": "provider to add",
                            "required": true,
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "properties": {
                                            "provider": {
                                                "$ref": "#/components/schemas/Provider"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "/provider/:prefix/:version": {
