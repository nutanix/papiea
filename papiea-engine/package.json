{
  "name": "papiea-engine",
  "version": "1.0.68",
  "description": "",
  "main": "build/main.js",
  "files": [
    "build/",
    "src/"
  ],
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc",
    "build!": "npm run clean && npm run build",
    "start": "ts-node src/main.ts",
    "test": "jest --runInBand --coverage",
    "test-ci": "jest --runInBand --coverage --ci --reporters=default --reporters=jest-junit",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "debug": "./node_modules/nodemon/bin/nodemon.js --config nodemon_debug.json",
    "test-watch": "jest --watchAll",
    "build-clj": "cd papiea-lib-clj && lein cljsbuild once",
    "build-clj-auto": "cd papiea-lib-clj && lein cljsbuild auto",
    "client": "ts-node examples/typescript/location/client/main.ts",
    "provider": "ts-node examples/typescript/location/provider/main.ts"
  },
  "author": "Shlomi Vaknin",
  "license": "ISC",
  "dependencies": {
    "@types/atob": "^2.1.2",
    "@types/btoa": "^1.2.3",
    "atob": "^2.1.2",
    "axios": "^0.18.1",
    "btoa": "^1.2.1",
    "casbin": "^2.0.1",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "express-async-handler": "^1.1.4",
    "express-swaggerize-ui": "^1.0.3",
    "js-yaml": "^3.12.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.13",
    "mongo-dot-notation-tool": "^1.0.1",
    "mongodb": "^3.1.10",
    "morgan": "^1.9.1",
    "papiea-core": "file:../papiea-core",
    "pluralize": "^7.0.0",
    "query-string": "^6.2.0",
    "simple-oauth2": "^2.2.1",
    "swagger-model-validator": "^3.0.6",
    "uuid": "^3.3.2",
    "uuid-validate": "0.0.3",
    "wait-port": "^0.2.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/js-yaml": "^3.11.4",
    "@types/lodash": "^4.14.123",
    "@types/mongodb": "^3.1.17",
    "@types/morgan": "^1.7.35",
    "@types/uuid": "^3.4.4",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.14.7",
    "@types/pluralize": "0.0.29",
    "@types/swagger-parser": "^4.0.2",
    "@types/uuid-validate": "0.0.0",
    "papiea-sdk": "file:../papiea-sdk",
    "fast-check": "^1.16.0",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "nodemon": "^1.18.8",
    "swagger-parser": "^6.0.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "\\.(test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**"
    ],
    "testEnvironment": "node"
  }
}
